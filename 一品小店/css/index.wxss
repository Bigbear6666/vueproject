* {
  margin: 0;
  padding: 0;
}
body {
  width: 100%;
  position: static;
  overflow-x: hidden;
  font-size: 12px;
  font-family: 微软雅黑;
  background: #ffffff;
}
#app {
  width: 100%;
  margin: auto;
}
.X-header {
  width: 1200px;
  margin: 30px auto;
  height: 34px;
  line-height: 34px;
}
.X-header a {
  text-decoration: none;
  width: 140px;
  height: 34px;
  font-size: 32px;
  font-weight: bold;
  color: #ff5600;
  float: left;
}
.X-header input {
  outline: none;
  vertical-align: middle;
}
.X-header input.first {
  float: left;
  width: 30%;
  height: 33px;
  border: 2px solid #e4393c;
  margin-left: 350px;
}
.X-header input.sec {
  width: 10%;
  height: 37px;
  background: #e4393c;
  font-size: 20px;
  color: #ffffff;
  border: none;
  cursor: pointer;
}
.X-nav {
  width: 100%;
  height: 40px;
  background: #4f4f4f;
  margin-top: 24px;
}
.nav_2 {
  width: 1200px;
  margin: auto;
}
.assort {
  width: 200px;
  height: 40%;
  background: #ff680b;
  float: left;
}
.assort h1 {
  color: #ffffff;
  font-size: 18px;
  line-height: 40px;
  text-align: center;
}
.nav_1 {
  width: 800px;
  height: 40px;
  float: left;
}
ul {
  list-style: none;
}
ul li {
  float: left;
  width: 100px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  margin-right: 3.75px;
  font-size: 16px;
}
ul li a {
  text-decoration: none;
  display: block;
  color: #ffffff;
}
ul li:nth-child(1) a {
  background: #ffffff;
  color: #ff5600;
}
ul li:hover a {
  display: block;
  background: #ffffff;
  color: #ff5600;
}
.main_banner {
  width: 1200px;
  height: 390px;
  margin: auto;
}
.fl {
  float: left;
}
.banner_left {
  width: 200px;
  height: 390px;
  background: #f5f5f5;
}
.banner_left ul li {
  height: 50px;
  line-height: 50px;
  margin-left: 50px;
  font-size: 15px;
}
.banner_left ul li a {
  color: #333;
  text-decoration: none;
  background: none;
}
.banner_left ul li:hover a {
  color: #ff5600;
}
.banner_right {
  width: 80%;
  height: 380px;
  margin-left: 10px;
  margin-top: 10px;
}
.banner_right img {
  width: 100%;
  height: 100%;
}
img {
  border: 0;
}
#main {
  width: 1200px;
  margin: auto;
}
.lfood {
  width: 1158px;
  height: 507px;
  margin: 20px auto 20px auto;
}
.lfood h1 {
  margin-bottom: 10px;
  font-size: 20px;
  line-height: 40px;
}
.lfood .food-con {
  width: 1158px;
}
.lfood .food-con .food-lf {
  float: left;
}
img {
  border: 0;
}
.lfood .food-con .food-rt {
  width: 749px;
  height: 455px;
  border: 1px solid #e4e4e4;
  border-left: none;
  float: right;
}
.lfood .food-con .food-rt .ff-rT {
  width: 749px;
  height: 174px;
  padding: 18px 0;
  border-bottom: 1px solid #e4e4e4;
}
.lfood .food-con .food-rt .ff-rT li {
  width: 372px;
  margin-right: 0;
}
.lfood .food-con .food-rt .ff-rT li a {
  display: block;
  width: 174px;
  float: left;
  margin-left: 15px;
}
.lfood .food-con .food-rt .ff-rT li .cmqkl {
  width: 160px;
  height: 174px;
  float: left;
  margin: 12px 11px;
}
.lfood .food-con .food-rt .ff-rT li .cmqkl a.qkl {
  font-size: 16px;
  font-weight: bold;
  color: #000104;
  margin: 25px 0 6px 0;
}
.lfood .food-con .food-rt .ff-rT li .cmqkl a.qkl:hover {
  background: #ffffff;
  color: #ff7f00;
}
.lfood .food-con .food-rt .ff-rT li .cmqkl p {
  color: #6c6a77;
  line-height: 150%;
  font-size: 12px;
}
.lfood .food-con .food-rt .ff-rB {
  width: 760px;
}
.lfood .food-con .food-rt .ff-rB li:last-child {
  border-right: 0;
}
.lfood .food-con .food-rt .ff-rB .csl {
  border-right: 1px solid #e4e4e4;
  float: left;
  width: 246.3px;
  height: 245px;
}
.lfood .food-con .food-rt .ff-rB .csl a img {
  display: block;
  margin: 10px 16px 5px 16px;
}
.lfood .food-con .food-rt .ff-rB .csl p {
  font-size: 12px;
}
.lfood .food-con .food-rt .ff-rB .csl p:hover {
  color: #ff7f00;
  cursor: pointer;
}
.lS {
  width: 1200px;
  height: 80px;
  border-bottom: 2px solid #e4e4e4;
}
.X-snack {
  width: 114px;
  height: 80px;
  border-bottom: 2px solid #ff7f00;
}
.X-snack p {
  font-size: 20px;
  line-height: 100px;
  text-align: center;
}
.X-ls-main {
  width: 1180px;
  height: 448px;
  margin: auto;
  margin-top: 20px;
}
.X-ls-main-lf {
  width: 250px;
}
ol {
  width: 250px;
  height: 143px;
  list-style: none;
}
ol li {
  height: 30px;
  float: left;
  margin-right: 33px;
  margin-top: 10px;
  line-height: 30px;
  font-size: 12px;
}
ol li a {
  text-decoration: none;
  color: black;
}
ol li a:hover {
  color: #ff7f00;
}
.X-ls-main p {
  width: 250px;
  font-size: 16px;
}
.X-ls-main-rt {
  width: 900px;
  height: 448px;
  background: #ffb700;
  margin-left: 280px;
  margin-top: -470px;
}
